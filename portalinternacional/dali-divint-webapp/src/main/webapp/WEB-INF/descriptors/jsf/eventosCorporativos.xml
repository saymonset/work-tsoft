	<!-- Definición del controlador para la administración de personas -->
	<managed-bean>
		<managed-bean-name>personasBean</managed-bean-name>
		<managed-bean-class>
			com.indeval.portalinternacional.presentation.controller.eventosCorporativos.catalogos.PersonaController
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>personaService</property-name>				
			<value>#{personaService}</value>
		</managed-property>
		 <managed-property>
            <property-name>consultaCatService</property-name>
            <value>#{consultaCatalogoService}</value>
        </managed-property>
        <managed-property>
            <property-name>listaDistribucionService</property-name>
            <value>#{listaDistribucionService}</value>
        </managed-property>
	</managed-bean>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>personasPDF</from-outcome>
			<to-view-id>/eventosCorporativos/catalogos/personasPDF.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>personasXLS</from-outcome>
			<to-view-id>/eventosCorporativos/catalogos/personasXLS.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	
	
	<!-- Definicion del controlador para la administracion de listas de distribucion -->
	<managed-bean>
		<managed-bean-name>listaDistribucionBean</managed-bean-name>
		<managed-bean-class>
			com.indeval.portalinternacional.presentation.controller.eventosCorporativos.catalogos.ListaDistribucionController
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		
		<managed-property>
			<property-name>listaDistribucionService</property-name>				
			<value>#{listaDistribucionService}</value>
		</managed-property>
	</managed-bean>
	<navigation-rule>
        <from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>listasDistribucionPDF</from-outcome>
			<to-view-id>/eventosCorporativos/catalogos/listasDistribucionPDF.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>listasDistribucionXLS</from-outcome>
			<to-view-id>/eventosCorporativos/catalogos/listasDistribucionXLS.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
