<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
              version="1.2">
    <!--Multidivisas: Descriptor :: Pantalla de Movimiento de Efectivo Internacional-->
    <managed-bean>
        <managed-bean-name>movimientoEfectivoInt</managed-bean-name>
        <managed-bean-class>
            com.indeval.portalinternacional.presentation.controller.movimientoInternacional.MovimientoEfectivoInternacionalController
        </managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>

        <managed-property>
            <property-name>movimientoEfectivoInternacionalService</property-name>
            <value>#{movimientoEfectivoInternacionalService}</value>
        </managed-property>

        <managed-property>
            <property-name>solicitudAutorizacionService</property-name>
            <value>#{solicitudAutorizacionService}</value>
        </managed-property>

        <managed-property>
            <property-name>divisaDao</property-name>
            <value>#{divisaDao}</value>
        </managed-property>

        <managed-property>
            <property-name>bovedaService</property-name>
            <value>#{bovedaService}</value>
        </managed-property>

        <managed-property>
            <property-name>sicService</property-name>
            <value>#{sicService}</value>
        </managed-property>

        <managed-property>
            <property-name>bitacoraService</property-name>
            <value>#{bitacoraService}</value>
        </managed-property>

    </managed-bean>

</faces-config>