<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd" version="1.2">
    <managed-bean>
    	<managed-bean-name>admonCustodiosDepositantesController</managed-bean-name>
    	<managed-bean-class>com.indeval.portalinternacional.presentation.controller.administracion.AdmonCustodiosDepositantesConstroller</managed-bean-class>
    	<managed-bean-scope>request</managed-bean-scope>
    	<managed-property>
            <property-name>divisionInternacionalService</property-name>
            <value>#{divisionInternacionalServiceEJB}</value>
        </managed-property>
        <managed-property>
            <property-name>consultaCatalogoService</property-name>
            <value>#{consultaCatalogoService}</value>
        </managed-property>
        <managed-property>
            <property-name>depositantesLiqBean</property-name>
            <value>#{admonCustodiosDepositantesDepLiqController}</value>
        </managed-property>
        <managed-property>
            <property-name>emisionesBean</property-name>
            <value>#{admonCustodiosDepositantesEmiController}</value>
        </managed-property>
        <managed-property>
            <property-name>consultaEmisionesBean</property-name>
            <value>#{admonCustodiosDepositantesConsultaEmiController}</value>
        </managed-property>
        <managed-property>
            <property-name>consultaDepositantesBean</property-name>
            <value>#{admonCustodiosDepositantesConsultaDepLiqController}</value>
        </managed-property>        
    </managed-bean>
    <managed-bean>
    	<managed-bean-name>admonCustodiosDepositantesDepLiqController</managed-bean-name>
    	<managed-bean-class>com.indeval.portalinternacional.presentation.controller.administracion.AdmonCustodiosDepositantesDepLiqConstroller</managed-bean-class>
    	<managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>consultaCatalogoService</property-name>
            <value>#{consultaCatalogoService}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
    	<managed-bean-name>admonCustodiosDepositantesEmiController</managed-bean-name>
    	<managed-bean-class>com.indeval.portalinternacional.presentation.controller.administracion.AdmonCustodiosDepositantesEmiConstroller</managed-bean-class>
    	<managed-bean-scope>request</managed-bean-scope>
    	<managed-property>
            <property-name>consultaCatalogoService</property-name>
            <value>#{consultaCatalogoService}</value>
        </managed-property>
        <managed-property>
			<property-name>sicEmisionDao</property-name>
			<value>#{sicEmisionDao}</value>
		</managed-property>
    </managed-bean>
    <managed-bean>
    	<managed-bean-name>admonCustodiosDepositantesConsultaEmiController</managed-bean-name>
    	<managed-bean-class>com.indeval.portalinternacional.presentation.controller.administracion.AdmonCustodiosDepositantesConsultaEmiController</managed-bean-class>
    	<managed-bean-scope>request</managed-bean-scope>
    	<managed-property>
            <property-name>consultaCatalogoService</property-name>
            <value>#{consultaCatalogoService}</value>
        </managed-property>
        <managed-property>    
            <property-name>divisionInternacionalService</property-name>
            <value>#{divisionInternacionalServiceEJB}</value>
        </managed-property>
        <managed-property>
			<property-name>registrosXPag</property-name>
			<value>#{50}</value>
		</managed-property>
		<managed-property>
			<property-name>sicEmisionDao</property-name>
			<value>#{sicEmisionDao}</value>
		</managed-property>
    </managed-bean>    
    <managed-bean>
    	<managed-bean-name>admonCustodiosDepositantesConsultaDepLiqController</managed-bean-name>
    	<managed-bean-class>com.indeval.portalinternacional.presentation.controller.administracion.AdmonCustodiosDepositantesConsultaDepLiqController</managed-bean-class>
    	<managed-bean-scope>request</managed-bean-scope>    	
        <managed-property>    
            <property-name>divisionInternacionalService</property-name>
            <value>#{divisionInternacionalServiceEJB}</value>
        </managed-property>
        <managed-property>
			<property-name>registrosXPag</property-name>
			<value>#{50}</value>
		</managed-property>
    </managed-bean>
    <navigation-rule>
    	<from-view-id>/administracion/admonCustodiosDepositantes.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>nuevaEmision</from-outcome>
    		<to-view-id>/administracion/admonCustodiosDepAltaEmision.xhtml</to-view-id>
    	</navigation-case>
    	<navigation-case>
    		<from-outcome>editarEmision</from-outcome>
    		<to-view-id>/administracion/admonCustodiosDepEdicionEmision.xhtml</to-view-id>
    	</navigation-case>
    	<navigation-case>
    		<from-outcome>buscarEmision</from-outcome>
    		<to-view-id>/administracion/admonCustodiosDepBuscarEmision.xhtml</to-view-id>
    	</navigation-case>
    	<navigation-case>
    		<from-outcome>nuevoDepositante</from-outcome>
    		<to-view-id>/administracion/admonCustodiosDepAltaDepositante.xhtml</to-view-id>
    	</navigation-case>
    	<navigation-case>
    		<from-outcome>buscarDepositante</from-outcome>
    		<to-view-id>/administracion/admonCustodiosDepBuscarDepositante.xhtml</to-view-id>
    	</navigation-case>
    	<navigation-case>
    		<from-outcome>editarDepositante</from-outcome>
    		<to-view-id>/administracion/admonCustodiosDepEdicionDepositante.xhtml</to-view-id>
    	</navigation-case>
    	<navigation-case>
    		<from-outcome>nuevoCustodio</from-outcome>
    		<to-view-id>/administracion/admonCustodiosDepAltaCustodio.xhtml</to-view-id>
    	</navigation-case>
    	<navigation-case>
    		<from-outcome>editarCustodio</from-outcome>
    		<to-view-id>/administracion/admonCustodiosDepEdicionCustodio.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<from-view-id>*</from-view-id>
    	<navigation-case>
    		<from-outcome>regresaAdmonCustDep</from-outcome>
    		<to-view-id>/administracion/admonCustodiosDepositantes.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
</faces-config>