<?xml version="1.0"?>
<!DOCTYPE facelet-taglib PUBLIC
  "-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
  "http://java.sun.com/dtd/facelet-taglib_1_0.dtd">

<facelet-taglib>
    <namespace>http://blitzsoftware.com/jsf/datetag</namespace>
    <tag>
        <tag-name>simpleDate</tag-name>
        <component>
            <component-type>indeval.Date</component-type>
            <renderer-type>indeval.Date</renderer-type>
        </component>
    </tag>
    
     <tag>
        <tag-name>initDate</tag-name>
        <component>
            <component-type>indeval.InitDate</component-type>
            <renderer-type>indeval.InitDate</renderer-type>
        </component>
    </tag>
    
     <tag>
        <tag-name>endDate</tag-name>
        <component>
            <component-type>indeval.EndDate</component-type>
            <renderer-type>indeval.EndDate</renderer-type>
        </component>
    </tag>
    
   
</facelet-taglib>