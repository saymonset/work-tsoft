<?xml version="1.0"?>
<!DOCTYPE facelet-taglib PUBLIC
  "-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
  "http://java.sun.com/dtd/facelet-taglib_1_0.dtd">

<facelet-taglib>
    <namespace>http://indeval.com/jsf/pdf</namespace>
    <tag>
        <tag-name>document</tag-name>
        <component>
            <component-type>indeval.PDFDocument</component-type>
            <renderer-type>indeval.PDFDocument</renderer-type>
        </component>
    </tag>
    <tag>
        <tag-name>filter</tag-name>
        <component>
            <component-type>indeval.PDFFilter</component-type>
            <renderer-type>indeval.PDFFilter</renderer-type>
        </component>
    </tag>
    <tag>
        <tag-name>col</tag-name>
        <component>
            <component-type>indeval.PDFColumn</component-type>
            <renderer-type>indeval.PDFColumn</renderer-type>
        </component>
    </tag>
    <tag>
        <tag-name>row</tag-name>
        <component>
            <component-type>indeval.PDFRow</component-type>
            <renderer-type>indeval.PDFRow</renderer-type>
        </component>
    </tag>
    <tag>
        <tag-name>table</tag-name>
        <component>
            <component-type>indeval.PDFTable</component-type>
            <renderer-type>indeval.PDFTable</renderer-type>
        </component>
    </tag>
    <tag>
        <tag-name>iterator</tag-name>
        <component>
            <component-type>indeval.PDFIterator</component-type>
            <renderer-type>indeval.PDFIterator</renderer-type>
        </component>
    </tag>
    <tag>
        <tag-name>subtitle</tag-name>
        <component>
            <component-type>indeval.PDFSubTitle</component-type>
            <renderer-type>indeval.PDFSubTitle</renderer-type>
        </component>
    </tag>
    <tag>
        <tag-name>newPage</tag-name>
        <component>
            <component-type>indeval.PDFNewPage</component-type>
            <renderer-type>indeval.PDFNewPage</renderer-type>
        </component>
    </tag>
    <tag>
        <tag-name>br</tag-name>
        <component>
            <component-type>indeval.PDFText</component-type>
            <renderer-type>indeval.PDFText</renderer-type>
        </component>
    </tag>
    <tag>
        <tag-name>text</tag-name>
        <component>
            <component-type>indeval.PDFText</component-type>
            <renderer-type>indeval.PDFText</renderer-type>
        </component>
    </tag>
    <tag>
        <tag-name>grid</tag-name>
        <component>
            <component-type>indeval.PDFGrid</component-type>
            <renderer-type>indeval.PDFGrid</renderer-type>
        </component>
    </tag>
    <tag>
        <tag-name>gridRow</tag-name>
        <component>
            <component-type>indeval.PDFGridRow</component-type>
            <renderer-type>indeval.PDFGridRow</renderer-type>
        </component>
    </tag>
    <tag>
        <tag-name>gridCol</tag-name>
        <component>
            <component-type>indeval.PDFGridColumn</component-type>
            <renderer-type>indeval.PDFGridColumn</renderer-type>
        </component>
    </tag>
</facelet-taglib>