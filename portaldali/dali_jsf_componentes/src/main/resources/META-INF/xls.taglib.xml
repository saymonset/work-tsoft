<?xml version="1.0"?>
<!DOCTYPE facelet-taglib PUBLIC
  "-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
  "http://java.sun.com/dtd/facelet-taglib_1_0.dtd">

<facelet-taglib>
    <namespace>http://indeval.com/jsf/xls</namespace>
    <tag>
        <tag-name>document</tag-name>
        <component>
            <component-type>indeval.XLSDocument</component-type>
            <renderer-type>indeval.XLSDocument</renderer-type>
        </component>
    </tag>
    <tag>
        <tag-name>subtitle</tag-name>
        <component>
            <component-type>indeval.XLSSubTitle</component-type>
            <renderer-type>indeval.XLSSubTitle</renderer-type>
        </component>
    </tag>
    <tag>
        <tag-name>col</tag-name>
        <component>
            <component-type>indeval.XLSColumn</component-type>
            <renderer-type>indeval.XLSColumn</renderer-type>
        </component>
    </tag>
    <tag>
        <tag-name>row</tag-name>
        <component>
            <component-type>indeval.XLSRow</component-type>
            <renderer-type>indeval.XLSRow</renderer-type>
        </component>
    </tag>
    <tag>
        <tag-name>table</tag-name>
        <component>
            <component-type>indeval.XLSTable</component-type>
            <renderer-type>indeval.XLSTable</renderer-type>
        </component>
    </tag>
    <tag>
        <tag-name>iterator</tag-name>
        <component>
            <component-type>indeval.XLSIterator</component-type>
            <renderer-type>indeval.XLSIterator</renderer-type>
        </component>
    </tag>
    <tag>
        <tag-name>br</tag-name>
        <component>
            <component-type>indeval.XLSBR</component-type>
            <renderer-type>indeval.XLSBR</renderer-type>
        </component>
    </tag>
    <tag>
        <tag-name>newSheet</tag-name>
        <component>
            <component-type>indeval.XLSNewSheet</component-type>
            <renderer-type>indeval.XLSNewSheet</renderer-type>
        </component>
    </tag>
    
</facelet-taglib>