<?xml version="1.0"?>
<!DOCTYPE facelet-taglib PUBLIC
  "-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
  "http://java.sun.com/dtd/facelet-taglib_1_0.dtd">

<facelet-taglib>
    <namespace>http://indeval.com/indeval</namespace>

    <tag>
        <tag-name>authorize</tag-name>
        <component>
        	<component-type>IndevalAutorizacionTag</component-type>
        </component>
    </tag>
    
    <tag>
        <tag-name>authorizeFaculty</tag-name>
        <component>
        	<component-type>IndevalAutorizacionFacultadTag</component-type>
        </component>
    </tag>
    
     <tag>
        <tag-name>captcha</tag-name>
        <component>
        	<component-type>CaptchaComponentTag</component-type>
        </component>
    </tag>
    
    <tag>
		<tag-name>captchaValidator</tag-name>
		<validator>
			<validator-id>captchaValidator</validator-id>
		</validator>	
	</tag>
    
    

</facelet-taglib>