<?xml version='1.0' encoding='UTF-8'?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd" version="1.2">

    <managed-bean>
        <managed-bean-name>archivoConciliacionBean</managed-bean-name>
        <managed-bean-class>com.indeval.portaldali.presentation.controller.mercadodinero.archivoConciliacion.ArchivoConciliacionController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>consultaCatalogosFacade</property-name>
			<value>#{consultaCatalogosFacade}</value>
		</managed-property>
		<managed-property>
			<property-name>registrosXPag</property-name>
			<value>20</value>
		</managed-property>
		<managed-property>
			<property-name>conciliacionService</property-name>
			<value>#{conciliacionService}</value>
		</managed-property>
		<managed-property>
			<property-name>archivoConciliacionMovimientosBean</property-name>
			<value>#{archivoConciliacionMovimientosBean}</value>
		</managed-property>
		<managed-property>
			<property-name>emisionDAO</property-name>
			<value>#{emisionDAO}</value>
		</managed-property>		
		<managed-property>
			<property-name>consultaPosicionService</property-name>
			<value>#{consultaPosicionService}</value>
		</managed-property>
		<managed-property>
			<property-name>idMercadoDinero</property-name>
			<value>#{identificadorMercadoDinero}</value>
		</managed-property>
		<managed-property>
			<property-name>utilService</property-name>
			<value>#{utilServices}</value>
		</managed-property>
		<managed-property>
			<property-name>consultaMovimientosValorService</property-name>
			<value>#{consultaMovimientosValorService}</value>
		</managed-property>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>archivoConciliacionMovimientosBean</managed-bean-name>
        <managed-bean-class>com.indeval.portaldali.presentation.controller.mercadodinero.archivoConciliacion.ArchivoConciliacionMovimientosController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>registrosXPag</property-name>
			<value>20</value>
		</managed-property>
		<managed-property>
			<property-name>consultaMovimientosValorService</property-name>
			<value>#{consultaMovimientosValorService}</value>
		</managed-property>
		<managed-property>
			<property-name>consultaCatalogosFacade</property-name>
			<value>#{consultaCatalogosFacade}</value>
		</managed-property>
		<managed-property>
			<property-name>utilService</property-name>
			<value>#{utilServices}</value>
		</managed-property>
    </managed-bean>
    
    <navigation-rule>
    	<from-view-id>/mercadoDinero/archivoConciliacion/archivoConciliacion.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>mostrarDetalle</from-outcome>
    		<to-view-id>/mercadoDinero/archivoConciliacion/archivoConciliacionDetalle.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<from-view-id>*</from-view-id>
    	<navigation-case>
    		<from-outcome>regresarConsultaArchivoConciliacion</from-outcome>
    		<to-view-id>/mercadoDinero/archivoConciliacion/archivoConciliacion.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
    <navigation-rule>
    	<from-view-id>/mercadoDinero/archivoConciliacion/archivoConciliacion.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>mostrarMovimiento</from-outcome>
    		<to-view-id>/mercadoDinero/archivoConciliacion/archivoConciliacionMovimientos.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
    
</faces-config>

