 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>inicio</from-outcome>
   <to-view-id>/index.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>changePassword</from-outcome>
   <to-view-id>/changePassword.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>header</from-outcome>
   <to-view-id>/header.xhtml</to-view-id>
  </navigation-case>
  <!-- Reglas de navegación Login -->
  <!-- <navigation-case>
   <from-action>#{inicioSesionBean.determinaLogin}</from-action>
   <from-outcome>login</from-outcome>
   <to-view-id>/login.xhtml</to-view-id>
  </navigation-case>
   <navigation-case>   
   <from-outcome>loginOk</from-outcome>
   <to-view-id>/index.xhtml</to-view-id>
  </navigation-case>-->
  
 
  
  
  
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>/login.xhtml</to-view-id>
  </navigation-case>
  
  <navigation-case>
   <from-outcome>changeOk</from-outcome>
   <to-view-id>/login.xhtml</to-view-id>
  </navigation-case>
  
  <navigation-case>
   <from-action>#{inicioSesionBean.logout}</from-action>
   <from-outcome>login</from-outcome>
   <to-view-id>/login.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  
  <navigation-case>
   <from-action>#{capturaOperacionesBean.cambiarOperacion}</from-action>
   <from-outcome>colocacionPrimaria</from-outcome>
   <to-view-id>/mercadoDinero/operacion/colocacionPrimaria.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-action>#{capturaOperacionesBean.cambiarOperacion}</from-action>
   <from-outcome>fondeoCtaPropia</from-outcome>
   <to-view-id>/mercadoDinero/operacion/fondeoCtaPropia.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-action>#{capturaOperacionesBean.cambiarOperacion}</from-action>
   <from-outcome>reportoNominal</from-outcome>
   <to-view-id>/mercadoDinero/operacion/reportoNominal.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-action>#{capturaOperacionesBean.cambiarOperacion}</from-action>
   <from-outcome>traspasoLibrePago</from-outcome>
   <to-view-id>/mercadoDinero/operacion/traspasoLibrePago.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-action>#{capturaOperacionesBean.cambiarOperacion}</from-action>
   <from-outcome>venta</from-outcome>
   <to-view-id>/mercadoDinero/operacion/venta.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
